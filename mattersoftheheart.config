	// This makes it so the family tree system has no restrictions on which NPCs you can and cannot date.
	"allowIncest" : false,
	
	// This makes it so all marriage restrictions, apart from relationship level, are removed. Multiple players can marry the same spouse, a specific player can marry many spouses. 
	// Otherwise, you may only marry one NPC at a time, and not someone else's spouse.
	"allowPolygamy" : false,
	
	// This makes relationships decay over time, needing the player to interact regularly with NPCs in order to keep a stable relationship level.
	"enableRelationshipDecay" : true,
	
	// This makes it so that NPCs have specific gender preferences. Otherwise, every NPC that exists will be pansexual.
	"enableGenderPreferences" : true,
	
	// If enableGenderPreferences is enabled, these are the odds of an npc having specific sexualities. These are weight-based; higher number equals higher chance.
	"genderPreferencesOdds" : [
		[ 55, "heterosexual" ],
		[ 25, "homosexual" ],
		[ 15, "pansexual" ],
		[ 5, "asexual" ]
	],
	
	// This makes it so that NPCs have randomly generated affinity towards the player. Otherwise, all NPCs will be neutral, or racist if the below is enabled.
	"enableRandomAffinity" : true,
	
	// This makes it so that NPCs have racial biases. Otherwise, racism won't exist.
	"enableRacialAffinity" : true,
	
	// If the two config options above are enabled, these two numbers affect which affinity has a larger influence.
