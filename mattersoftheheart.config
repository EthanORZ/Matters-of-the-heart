	// These are the arbitrary values given by the various actions you can do while flirting.
	// A heart is 10 for reference.
	// 0 and lower is equal to infinite for cooldowns and counts.
	"flirtingValues" : {
		// The max amount of love points you can have at any given moment.
		"heartsCap" : 100,
		// When married, the max amount of love points you can have over the regular hearts cap. This needs to be filled for procreation, and empties upon producing offspring.
		"overHeartsCap" : 40,
		// The max amount of negative love points you can have at any given moment.
		"evilHeartsCap" : -100,
		
		// The followings are the love point milestones you have to reach in order to qualify for the statuses.
		"AnnoyanceCap" : -40,
		"FriendCap" : 30,
		"LoverCap" : 70,
		"MarryCap" : 100,
		
		// The time at which relationship decay starts.
		"decayStart" : 5400,
		// The interval at which decay decreases relationship.
		"decayInterval" : 600,
		// The amount of relationship points decay removes per interval.
		"decayValue" : -2,
	
		// The amount of love attributed by a normal chat.
		"interactionChatValue" : 0.2,
		// The amount of love attributed by a rightly answered question.
		"questionRightValue" : 1,
		// The amount of love attributed by a wrongly answered question.
		"questionWrongValue" : -1,
		// The chance to unlock a random data value in the NPC each interaction.
		// 0 is never, 1 is guaranteed. Must be between 0 or 1 or else it will crash.
		// Rolled 0 times for questions answered wrongly;
		// Rolled 1 time for normal chatter;
		// Rolled 2 times for questions answered right.
		"interactionUnlockChance" : 0.05,
		// The amount of times you can interact before it goes on cooldown.
		"interactionCount" : 3,
		// The cooldown time in seconds for interactions.
		"interactionCooldown" : 300,
		
		// These are the maximum possible values of gifts, before affinity multipliers.
		// From neutral to loved, higher quality increases the value of gifts.
		// From disliked to hated, higher quality decreases the negative value of gifts.
		"giftLovedValue" : 10,
		"giftLikedValue" : 4,
		"giftNeutralValue" : 1,
		"giftDislikedValue" : -4,
		"giftHatedValue" : -10,
		// The amount of times you can gift before it goes on cooldown.
		"giftCount" : 1,
		// The cooldown time in seconds for gifts.
		"giftCooldown" : 900,
		
		// The amount of times you can procreate before it goes on cooldown.
		"procreateCount" : 1,
		// The cooldown time in seconds for succesful procreation.
		"procreateCooldown" : 3600,
		"procreateCost" : 1.0, //100% of overhearts get consumed on success
		// The cooldown time in seconds for failed procreation.
		"procreateFailCooldown" : 300,
		"procreateFailCost" : 0.3 //30% of overhearts get consumed on failure
